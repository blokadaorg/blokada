<?xml version="1.0"?>
<resources>
    <string name="notification_on_text">Notifications</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_on_desc">Wenn aktiviert, zeigt %s eine Benachrichtigung für jede blockierte Anzeige.</string>
    <!-- Title of the notification that shows up every time ad is blocked. -->
    <string name="notification_blocked_title">"Anzeige blockiert "</string>
    <!-- Content of the notification that shows up every time ad is blocked. Parameter: blocked host name. -->
    <string name="notification_blocked_text">%s</string>
    <!-- Button to turn off notifications. -->
    <string name="notification_blocked_off">Nicht anzeigen</string>
    <!-- Button to whitelist a host that was just blocked, visible in a notification. -->
    <string name="notification_blocked_whitelist">Whitelist</string>
    <!-- Toast message that shows up once user adds a host to the whitelist, using a button from the notification. -->
    <string name="notification_blocked_whitelist_applied">Fertig! Die Änderungen werden in wenigen Sekunden aktiv.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_disabled">%s wird keine Notifications anzeigen.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_enabled">%s zeigt eine Benachrichtigung für jede blockierte Anzeige.</string>
    <string name="notification_keepalive_text">Aktiv halten</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_desc">Wenn aktiv, benutzt %s eine Methode, um bei Speicherbereinigungen aktiv zu bleiben.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_disabled">%s ist nicht mehr vor Akku- oder Speicheroptimierungen geschützt.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_enabled">%s verhindert nun eine systemseitige Deaktivierung.</string>
    <!-- The ads blocked counter, displayed in the Keep Alive notification. Parameter: ads blocked counter. -->
    <plurals name="notification_keepalive_title">
        <item quantity="one">Eine Anzeige blockiert</item>
        <item quantity="other">%d Anzeigen blockiert</item>
    </plurals>
    <!-- The most recently blocked host, displayed in the Keep Alive notification. Parameter: blocked host name -->
    <string name="notification_keepalive_content">Zuletzt: %s</string>
    <!-- Content of the Keep Alive notification, if nothing was blocked yet, as in "Last: nothing yet". -->
    <string name="notification_keepalive_none">noch nichts</string>
</resources>
