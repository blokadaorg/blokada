    <string name="account_upgrade_cloud_description">Tato funkce je součástí služby Blokada Plus. Chcete-li na ní upgradovat, kontaktujte prosím naši zákaznickou podporu.</string>
    <string name="payment_header_activated">Aktivováno!</string>
    <string name="payment_activated_description">Blokada se nyní přepne do režimu Plus a připojí se přes jedno z našich zabezpečených míst. Pokud si nejste jisti, které z nich si vybrat, doporučujeme vám to nejbližší.</string>
    <string name="payment_action_choose_location">Zvolte umístění</string>
    <string name="payment_action_see_locations">Naše poloha</string>
    <string name="payment_title">Upgradujte na naši VPN službu, abyste měli kontrolu nad *vaším soukromím*.</string>
    <string name="payment_feature_title_change_location">Změna polohy</string>
    <string name="payment_feature_desc_change_location">Skryje vaši IP adresu a předstírá, že se nacházíte v jiné zemi. To vás chrání před třetími stranami a pomůže ochránit vaši identitu.</string>
    <string name="payment_feature_title_encrypt_data">Šifruje data</string>
    <string name="payment_feature_desc_encrypt_data">Data odesílaná pomocí našeho VPN tunelu jsou šifrována pomocí silných algoritmů, aby byla chráněna před odposlechem neoprávněných stran.</string>
    <string name="payment_feature_title_encrypt_dns">Šifrovat DNS</string>
    <string name="payment_feature_desc_encrypt_dns">Zlepšete své soukromí pomocí šifrování DNS. Blokada Cloud využívá moderní protokoly pro zachování vašeho soukromí.</string>
    <string name="payment_feature_title_performance">Skvělý výkon</string>
    <string name="payment_feature_desc_performance">Udržujte své zařízení svižné a internetové připojení na maximální rychlosti díky našemu novému cloudovému řešení.</string>
    <string name="payment_feature_title_battery">Nulový dopad na baterii</string>
    <string name="payment_feature_desc_battery">Výdrž baterie nebude ovlivněna, může se dokonce zlepšit, když je Blokada Cloud aktivována.</string>
    <string name="payment_feature_title_devices_cloud">Více zařízení</string>
    <string name="payment_feature_desc_devices_cloud">Nakonfigurujte a sledujte všechna svá zařízení na jednom místě. Buď prostřednictvím naší mobilní aplikace nebo naší webové aplikace.</string>
    <string name="payment_feature_title_cloud_vpn">Cloud plus VPN</string>
    <string name="payment_feature_desc_cloud_vpn">Kromě všech výhod služby Blokada Cloud, získáte také přístup k naší globální síti VPN</string>
    <string name="payment_feature_title_faster_connection">Rychlejší připojení</string>
    <string name="payment_feature_desc_faster_connection">Skvělá rychlost až 100 Mbps se servery v různých částech světa.</string>
    <string name="payment_feature_title_devices">Až 5 zařízení</string>
    <string name="payment_feature_desc_devices">Pomocí stejného účtu můžete sdílet své předplatné až s 5 zařízeními: iOS, Android nebo PC.</string>
    <string name="payment_feature_title_no_ads">Blokovat reklamy</string>
    <string name="payment_feature_desc_no_ads">Použijte populární technologii blokování reklam Blokada k blokování reklam na vašich zařízeních. K dispozici jsou i pokročilá nastavení.</string>
    <string name="payment_feature_title_support">Skvělá podpora</string>
    <string name="payment_feature_desc_support">Získejte rychlou odpověď na jakékoliv dotazy díky naší zákaznické podpoře a aktivní početné komunitě.</string>
