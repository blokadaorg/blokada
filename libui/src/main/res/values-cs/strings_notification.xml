<?xml version="1.0"?>
<resources>
    <string name="notification_on_text">Oznámení</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_on_desc">Pokud je povoleno, %s zobrazí oznámení pokaždé když je reklama blokována.</string>
    <!-- Title of the notification that shows up every time ad is blocked. -->
    <string name="notification_blocked_title">Reklama zablokována</string>
    <!-- Content of the notification that shows up every time ad is blocked. Parameter: blocked host name. -->
    <string name="notification_blocked_text">%s</string>
    <!-- Button to turn off notifications. -->
    <string name="notification_blocked_off">Nezobrazovat</string>
    <!-- Button to whitelist a host that was just blocked, visible in a notification. -->
    <string name="notification_blocked_whitelist">Whitelist</string>
    <!-- Toast message that shows up once user adds a host to the whitelist, using a button from the notification. -->
    <string name="notification_blocked_whitelist_applied">Hotovo! Změna může trvat několik sekund.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_disabled">%s nebude zobrazovat oznámení reklamy.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_enabled">%s bude zobrazovat oznámení pokaždé. když je reklama blokována.</string>
    <string name="notification_keepalive_text">Udržet naživu</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_desc">Pokud je povoleno, %s bude používat techniku, která pomáhá k lepší paměti.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_disabled">%s není chráněno před využitím baterie a pamětí.</string>
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_enabled">%s ochrana proti deaktivaci operačním systémem.</string>
    <!-- The ads blocked counter, displayed in the Keep Alive notification. Parameter: ads blocked counter. -->
    <plurals name="notification_keepalive_title">
        <item quantity="one">Jedna reklama zablokována</item>
        <item quantity="other">%d reklam zablokováno</item>
        <item quantity="few">%d reklamy zablokovány</item>
        <item quantity="many">%d reklam zablokováno</item>
    </plurals>
    <!-- The most recently blocked host, displayed in the Keep Alive notification. Parameter: blocked host name -->
    <string name="notification_keepalive_content">Poslední: %s</string>
    <!-- Content of the Keep Alive notification, if nothing was blocked yet, as in "Last: nothing yet". -->
    <string name="notification_keepalive_none">zatím nic</string>
</resources>
