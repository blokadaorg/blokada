    <string name="update_notification_text">A versão %1$s está pronta para ser descarregada! Prima para continuar.</string>
    <!-- Header displaying the new app version. Important! Parameter was removed from this string, so please remove any variable placeholders from translations. -->
    <string name="update_notification_channel">Notificações de atualização</string>
    <string name="update_info">Se descarregou a aplicação da Play Store, F-Droid ou outra loja de aplicações, irá receber atualizações a partir da respetiva loja. Certifique-se de que o Blokada se encontra desativado durante a atualização. Se encontrar algum problema na atualização, aceda a blokada.org e descarregue a aplicação manualmente. Não há necessidade de desinstalar a versão existente.</string>
    <!-- Displayed in the home screen when there is an update. Keep it short. -->
    <string name="update_dash_available">&lt;b&gt;Atualização disponível!&lt;/b&gt;</string>
    <!-- Displayed in the home screen whenever there is no update. Keep it short. -->
    <!-- Displayed in menu to open the About/Update screen. -->
    <!-- A button that opens system preferences for this app. -->
    <string name="update_button_appinfo">Detalhes da aplicação</string>
    <!-- A button that opens the changelog. -->
    <!-- A toast message displayed when the update process is starting. -->
    <string name="update_starting">A iniciar, por favor aguarde&#8230;</string>
    <string name="update_progress">A transferir: %s%%</string>
    <string name="update_complete">Transferência concluída</string>
    <string name="update_failed">Transferência falhou</string>
    <string name="update_instruction">Quando terminar, será solicitado para permitir que o Blokada instale a atualização.</string>
    <string name="update_instruction_failed">Pressione aqui para tentar novamente, ou pressione e aguente para transferir através do seu navegador.</string>
