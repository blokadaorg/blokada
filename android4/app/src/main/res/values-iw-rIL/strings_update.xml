<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="update_button">הורד עדכון</string>
    <string name="update_notification_title">עדכון זמין</string>
    <!-- Parameter: new app version, for example 2.0.1 -->
    <string name="update_notification_text">גירסא %1$s זמינה להורדה! לחץ על מנת להמשיך.</string>
    <!-- Header displaying the new app version. Important! Parameter was removed from this string, so please remove any variable placeholders from translations. -->
    <string name="update_notification_channel">התראות עדכון</string>
    <string name="update_info">אם הורדת את היישום מחנות ה-Play Store, F-Droid או כל חנות אחרת, תקבל התראה מהחנות כאשר העדכון יהיה זמין להורדה. וודא כי Blokada מכובית במהלך העדכון. אם הינן חווה בעיות בעדכון, עבור אל Blokada.org והורד את היישום באופן עצמאי. אין צורך להסיר את הגרסה הנוכחית לפניכן.</string>
    <!-- Displayed in the home screen when there is an update. Keep it short. -->
    <string name="update_dash_available">&lt;b&gt;עדכון זמין!&lt;/b&gt;</string>
    <!-- Displayed in the home screen whenever there is no update. Keep it short. -->
    <!-- Displayed in menu to open the About/Update screen. -->
    <!-- A button that opens system preferences for this app. -->
    <string name="update_button_appinfo">פרטי האפליקציה</string>
    <!-- A button that opens the changelog. -->
    <!-- A toast message displayed when the update process is starting. -->
    <string name="update_starting">מתחיל, אנא המתן&#8230;</string>
    <string name="update_progress">מוריד %s%%</string>
    <string name="update_complete">ההורדה הושלמה</string>
    <string name="update_failed">ההורדה נכשלה</string>
    <string name="update_instruction">כאשר התהליך יסתיים, תתבקש/י לאשר לBlokada להתקין את העדכון.</string>
    <string name="update_instruction_failed">לחצ/י כאן על מנת לנסות שנית, או לחצ/י לחיצה ארוכה על מנת להוריד עם הדפדפן שלך במקום.</string>
