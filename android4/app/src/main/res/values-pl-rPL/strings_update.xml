<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="update_button">Pobierz aktualizację</string>
    <string name="update_notification_title">Aktualizacja dostępna</string>
    <!-- Parameter: new app version, for example 2.0.1 -->
    <string name="update_notification_text">Wersja %1$s jest gotowa do pobrania. Wybierz, aby kontynuować.</string>
    <!-- Header displaying the new app version. Important! Parameter was removed from this string, so please remove any variable placeholders from translations. -->
    <string name="update_notification_channel">Powiadomienia aktualizacji</string>
    <string name="update_info">Jeśli pobrałeś/aś Blokadę z Play, F-Droid, lub innego sklepu, aktualizacje otrzymasz właśnie stamtąd. Blokada powinna być nieaktywna podczas aktualizacji. Jeśli masz jakikolwiek problem, przejdź do blokada.org i pobierz aplikację ręcznie. Nie ma potrzeby odinstalowywać istniejącej wersji.</string>
    <!-- Displayed in the home screen when there is an update. Keep it short. -->
    <string name="update_dash_available">&lt;b&gt;Dostępna aktualizacja!&lt;/b&gt;</string>
    <!-- Displayed in the home screen whenever there is no update. Keep it short. -->
    <!-- Displayed in menu to open the About/Update screen. -->
    <!-- A button that opens system preferences for this app. -->
    <string name="update_button_appinfo">Szczegóły aplikacji</string>
    <!-- A button that opens the changelog. -->
    <!-- A toast message displayed when the update process is starting. -->
    <string name="update_starting">Aktualizuję, proszę czekać&#8230;</string>
    <string name="update_progress">Pobieranie: %s%%</string>
    <string name="update_complete">Pobieranie ukończone</string>
    <string name="update_failed">Pobieranie nieudane</string>
    <string name="update_instruction">Po zakończeniu zostaniesz poproszony o zezwolenie Blokadzie na zainstalowanie aktualizacji.</string>
    <string name="update_instruction_failed">Stuknij tutaj, aby spróbować ponownie, lub stuknij długo, aby pobrać za pomocą przeglądarki.</string>
