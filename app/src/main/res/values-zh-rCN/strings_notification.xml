<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="notification_on_text">显示通知</string>
    <string name="notification_on_description">当开启时，Blokada将在每次屏蔽某件事时显示通知。 请注意，这与某些设备显示的VPN通知不同，不能关闭。&lt;br/&gt;您可以配置哪些通知应该显示在下面的设置中。</string>
    <!-- Title of the notification that shows up every time something is blocked. -->
    <string name="notification_blocked_title">广告已拦截</string>
    <!-- Content of the notification that shows up every time something is blocked. Parameter: blocked host name. -->
    <string name="notification_blocked_text">%s</string>
    <!-- Button to turn off notifications. -->
    <string name="notification_blocked_off">以后不通知该域名的拦截</string>
    <!-- Button to whitelist a host that was just blocked, visible in a notification. -->
    <string name="notification_blocked_whitelist">加入白名单</string>
    <!-- Toast message that shows up once user adds a host to the whitelist, using a button from the notification. -->
    <string name="notification_blocked_whitelist_applied">已完成！等待几秒以使其生效。</string>
    <!-- Parameter: branded app name (Blokada). -->
    <!-- Parameter: branded app name (Blokada). -->
    <string name="notification_keepalive_text">常驻通知栏</string>
    <string name="notification_keepalive_description">如果应用在后台处于活动状态时遇到问题，请激活此开关。 Blokada将启用永久性通知以防止系统被杀死；您可以通过选择通知上方和设置隐藏。</string>
    <!-- The blocked counter, displayed in the Keep Alive notification. -->
    <string name="notification_keepalive_title_new">保持滚动(点击隐藏)</string>
    <string name="notification_keepalive_text_new">Android 需要显示通知才能保持应用运行，但您可以在系统设置中自由禁用它。</string>
    <!-- The blocked counter, displayed in the Keep Alive notification. Parameter: blocked counter. -->
    <string name="notification_keepalive_title">%d已拦截</string>
    <!-- The most recently blocked host, displayed in the Keep Alive notification. Parameter: blocked host name -->
    <string name="notification_keepalive_content">最后拦截：%s</string>
    <!-- Content of the Keep Alive notification, if nothing was blocked yet, as in "Last: nothing yet". -->
    <string name="notification_keepalive_none">监测</string>
    <string name="notification_keepalive_activate">启用</string>
    <string name="notification_keepalive_activating">Blokada 正在启动。这可能需要一段时间。</string>
    <string name="notification_keepalive_deactivate">停用</string>
    <string name="notification_keepalive_deactivating">Blokada 已停用！</string>
    <string name="notification_channel_filtered">广告屏蔽</string>
    <string name="notification_channel_announcements">公告</string>
    <string name="notification_channel_other">其它</string>
</resources>
