    <string name="payment_title">Upgrade naar onze VPN-service om de controle over *uw privacy* te behouden.</string>
    <string name="payment_feature_title_change_location">Verander de locatie</string>
    <string name="payment_feature_desc_change_location">Verberg je IP-adres en doe alsof je in een ander land bent. Dit verbergt je van derden en helpt je je identiteit te beschermen.</string>
    <string name="payment_feature_title_encrypt_data">Gegevens versleutelen</string>
    <string name="payment_feature_desc_encrypt_data">Gegevens die via onze VPN-tunnel worden verzonden zijn versleuteld met sterke algoritmen om te beschermen tegen interceptie door onbevoegde partijen.</string>
    <string name="payment_feature_title_encrypt_dns">DNS versleutelen</string>
    <string name="payment_feature_desc_encrypt_dns">Verbeter uw privacy met DNS-versleuteling. Blokada gebruikt moderne protocollen om uw verkeer privé te houden.</string>
    <string name="payment_feature_title_performance">Grote prestaties</string>
    <string name="payment_feature_desc_performance">Houd je apparaat netjes en je internetverbinding op maximale snelheden, dankzij onze nieuwe Cloud oplossing.</string>
    <string name="payment_feature_title_battery">Zero Batterij Impact</string>
    <string name="payment_feature_desc_battery">Uw batterijduur zal niet worden beïnvloed, het kan zelfs verbeteren wanneer Blokada is geactiveerd.</string>
    <string name="payment_feature_title_devices_cloud">Meerdere apparaten</string>
    <string name="payment_feature_desc_devices_cloud">Configureer en monitor al uw apparaten op één plek. Via onze mobiele app of onze webapp.</string>
    <string name="payment_feature_title_cloud_vpn">De Cloud plus VPN</string>
    <string name="payment_feature_desc_cloud_vpn">Naast alle voordelen van Blokada Cloud krijgt u toegang tot ons wereldwijde VPN-netwerk</string>
    <string name="payment_feature_title_faster_connection">Sneller verbinding</string>
    <string name="payment_feature_desc_faster_connection">Geweldige snelheid tot 100 Mbps met servers in verschillende delen van de wereld.</string>
