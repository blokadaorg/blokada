<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="update_button">Stiahnuť aktualizáciu</string>
    <string name="update_notification_title">Aktualizácia je k dispozícii</string>
    <!-- Parameter: new app version, for example 2.0.1 -->
    <string name="update_notification_text">Zostava %1$s je pripravená na stiahnutie! Ťuknite pre pokračovanie.</string>
    <!-- Header displaying the new app version. Important! Parameter was removed from this string, so please remove any variable placeholders from translations. -->
    <string name="update_notification_channel">Upozornenia o aktualizácii</string>
    <string name="update_info">Ak ste si stiahli aplikáciu z Obchodu Play, F-Droid alebo z iného obchodu, dostanete aktualizáciu z obchodu hneď ako bude k dispozícii. Uistite sa, že počas aktualizácie je Blokada deaktivovaná. Ak sa vyskytnú akékoľvek problémy s aktualizáciou, prejdite na blokada.org a stiahnite aplikáciu ručne. Nie je potrebné odinštalovať existujúcu verziu.</string>
    <!-- Displayed in the home screen when there is an update. Keep it short. -->
    <string name="update_dash_available">&lt;b&gt;Aktualizácia je k dispozícii!&lt;/b&gt;</string>
    <!-- Displayed in the home screen whenever there is no update. Keep it short. -->
    <!-- Displayed in menu to open the About/Update screen. -->
    <!-- A button that opens system preferences for this app. -->
    <string name="update_button_appinfo">Podrobnosti o aplikácii</string>
    <!-- A button that opens the changelog. -->
    <!-- A toast message displayed when the update process is starting. -->
    <string name="update_starting">Spúšťanie, prosím, čakajte&#8230;</string>
    <string name="update_progress">Sťahovanie: %s%%</string>
    <string name="update_complete">Sťahovanie dokončené</string>
    <string name="update_failed">Sťahovanie zlyhalo</string>
    <string name="update_instruction">Po dokončení budete vyzvaní k inštalácii aktualizácie Blokady.</string>
    <string name="update_instruction_failed">Dotykom skúste znovu, príp. dotykom a podržaním stiahnete aktualizáciu s pomocou prehliadača.</string>
</resources>
